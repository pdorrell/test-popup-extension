{
  "name": "Test Popup",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },  
  "browser_action": {
    "default_title": "Test Popup"
  }, 
  "web_accessible_resources":
    [
      "popup.html", 
      "jquery-1.10.2.js", 
      "send-message-from-popup.js"
    ]
}
